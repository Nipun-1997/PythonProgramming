def absolutePermutation(n, k):
    # Write your code here
    new_list=[0]*n
    for number in range(1,n+1):
        #print(number)
        
        if number <=k:
            
            index_value=(number-(-k))-1
            
       
            
      
        else:
            
            index_value=number-k-1
          
        if index_value>=len(new_list) or new_list[index_value]!=0 :
            #print("Here")
            return -1
        else:
            #print(number)
            new_list[index_value]=number
    return new_list  
